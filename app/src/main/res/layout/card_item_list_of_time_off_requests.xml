<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="100dp"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:elevation="10dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    card_view:cardCornerRadius="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        card_view:layout_constraintBottom_toTopOf="@+id/textView11"
        card_view:layout_constraintTop_toTopOf="@+id/textView11">

        <TextView
            android:id="@+id/list_of_time_off_requests_start_date"
            android:layout_width="76dp"
            android:layout_height="18dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="18dp"
            android:text="2020-20-20"
            card_view:layout_constraintEnd_toEndOf="@+id/textView7"
            card_view:layout_constraintStart_toEndOf="@+id/textView10"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="19dp"
            android:layout_height="19dp"
            android:layout_marginTop="16dp"
            android:text="-"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_end_date"
            card_view:layout_constraintStart_toEndOf="@+id/textView10"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/list_of_time_off_requests_end_date"
            android:layout_width="0dp"
            android:layout_height="19dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="161dp"
            android:text="2020-20-20"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toEndOf="@+id/textView6"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/list_of_time_off_requests_start_time"
            android:layout_width="75dp"
            android:layout_height="19dp"
            android:text="20:20:20"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/textView7"
            card_view:layout_constraintStart_toEndOf="@+id/textView11"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.518" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="242dp"
            android:text="-"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toEndOf="@+id/list_of_time_off_requests_start_time"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.512" />

        <TextView
            android:id="@+id/list_of_time_off_requests_end_time"
            android:layout_width="93dp"
            android:layout_height="18dp"
            android:text="20:20:20"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.517"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.524" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="92dp"
            android:text="Date:"
            android:textStyle="bold"
            card_view:layout_constraintEnd_toStartOf="@+id/textView6"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="45dp"
            android:layout_height="19dp"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="13dp"
            android:text="Time:"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_start_time"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.493" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>