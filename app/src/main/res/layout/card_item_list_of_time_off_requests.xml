<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent" android:layout_height="100dp"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:elevation="10dp"
    android:layout_marginLeft="5dp"
    android:layout_marginRight="5dp"
    android:layout_marginTop="5dp"
    card_view:cardCornerRadius="5dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/list_of_time_off_requests_start_date"
            android:layout_width="73dp"
            android:layout_height="19dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="13dp"
            android:layout_marginBottom="18dp"
            android:text="2020-20-20"
            card_view:layout_constraintBottom_toTopOf="@+id/list_of_time_off_requests_start_time"
            card_view:layout_constraintEnd_toStartOf="@+id/textView6"
            card_view:layout_constraintStart_toEndOf="@+id/textView10"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="26dp"
            android:layout_height="17dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="13dp"
            android:layout_marginBottom="17dp"
            android:text="-"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toTopOf="@+id/textView7"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_end_date"
            card_view:layout_constraintStart_toEndOf="@+id/list_of_time_off_requests_start_date"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/list_of_time_off_requests_end_date"
            android:layout_width="77dp"
            android:layout_height="19dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="96dp"
            android:layout_marginBottom="17dp"
            android:text="2020-20-20"
            card_view:layout_constraintBottom_toTopOf="@+id/list_of_time_off_requests_end_time"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toEndOf="@+id/textView6"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/list_of_time_off_requests_start_time"
            android:layout_width="64dp"
            android:layout_height="18dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="16dp"
            android:text="20:20:20"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/textView7"
            card_view:layout_constraintStart_toEndOf="@+id/textView11"
            card_view:layout_constraintTop_toBottomOf="@+id/list_of_time_off_requests_start_date" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="21dp"
            android:layout_height="17dp"
            android:layout_marginEnd="9dp"
            android:layout_marginBottom="18dp"
            android:text="-"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_end_time"
            card_view:layout_constraintStart_toEndOf="@+id/list_of_time_off_requests_start_time"
            card_view:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/list_of_time_off_requests_end_time"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginEnd="115dp"
            android:layout_marginBottom="16dp"
            android:text="20:20:20"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toEndOf="@+id/textView7"
            card_view:layout_constraintTop_toBottomOf="@+id/list_of_time_off_requests_end_date" />

        <TextView
            android:id="@+id/textView10"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginStart="41dp"
            android:layout_marginTop="29dp"
            android:layout_marginEnd="19dp"
            android:layout_marginBottom="18dp"
            android:text="Date:"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toTopOf="@+id/textView11"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_start_date"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="50dp"
            android:layout_height="21dp"
            android:layout_marginStart="41dp"
            android:layout_marginEnd="15dp"
            android:layout_marginBottom="16dp"
            android:text="Time:"
            android:textStyle="bold"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toStartOf="@+id/list_of_time_off_requests_start_time"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/textView10" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>